    "jest-mock-axios": "^1.0.21",
    "moment": "^2.22.0",
    "native-base": "^2.4.1",
    "react": "16.0.0",
    "react-dom": "^16.3.1",
    "react-native": "0.50.3",
    "react-native-material-dropdown": "^0.6.1",
    "react-native-material-textfield": "^0.12.0",
    "react-navigation": "^1.5.11",
    "react-redux": "^5.0.7",
