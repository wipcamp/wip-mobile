{
  "name": "wip-mobile",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "jest-expo": "23.0.0",
    "react-native-scripts": "1.8.1",
    "react-test-renderer": "^16.3.1"
  },
  "main": "./node_modules/react-native-scripts/build/bin/crna-entry.js",
